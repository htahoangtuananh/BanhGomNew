{
  "expo": {
    "name": "BanhGom",
    "slug": "BanhGom",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/animated_logo.png",
    "scheme": "banhgom",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "buildNumber": "4",
      "supportsTablet": true,
      "infoPlist": {
        "NSFaceIDUsageDescription": "We need to use Face ID to securely log you in",
        "NSPhotoLibraryUsageDescription": "We need access to your photo library so that you can upload and share images for products.",
        "NSCameraUsageDescription": "We need access to your camera so that you can take photos for face checkin function to work properly.",
        "NSLocationWhenInUseUsageDescription": "We need access to your location to get your current location for checkin function to work properly.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.htahoangtuananh.banhgom",
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/animated_logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.htahoangtuananh.banhgom"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/animated_logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/animated_logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bbd8c01d-85c5-40b9-90b1-c83a299b757e"
      }
    },
    "owner": "htahoangtuananh"
  }
}
